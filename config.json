{
  "engine": {
    "name": "RPG Engine v2.0",
    "version": "2.0.0-alpha",
    "target_fps": 60,
    "max_frame_time": 0.016,
    "debug_mode": true
  },
  "ui": {
    "title": "RPG Engine - Interactive UI",
    "width": 1200,
    "height": 800,
    "theme": "default",
    "auto_scroll_logs": true,
    "max_log_lines": 1000
  },
  "logging": {
    "level": "INFO",
    "console_output": true,
    "file_output": true,
    "file_name": "rpg_engine.log",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  },
  "services": {
    "ui_enabled": true,
    "event_bus_history_size": 1000,
    "service_container_debug": false
  },
  "development": {
    "hot_reload": false,
    "profiling_enabled": false,
    "test_mode": false
  }
}
